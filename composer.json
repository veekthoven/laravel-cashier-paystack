{
    "name": "veekthoven/laravel-cashier-paystack",
    "description": "A Laravel Cashier Wrapper that provides an expressive, fluent interface to Paystack's subscription invoicing services.",
    "keywords": ["laravel", "paystack", "billing", "cashier"],
    "license": "MIT",
    "authors": [
        {
            "name": "Victor Abbah Nkoms",
            "email": "holla@veekthoven.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "dompdf/dompdf": "^2.0",
        "guzzlehttp/guzzle": "^7.8",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/http": "^10.0|^11.0",
        "illuminate/routing": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/view": "^10.0|^11.0",
        "nesbot/carbon": "^2.0|^3.0",
        "symfony/http-kernel": "^6.0|^7.0"
    },
    "require-dev": {
        "laravel/pint": "^1.13",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^8.18|^9.0",
        "pestphp/pest": "^2.24"
    },
    "autoload": {
        "psr-4": {
            "veekthoven\\Cashier\\": "src/",
            "veekthoven\\Cashier\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "veekthoven\\Cashier\\CashierServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
